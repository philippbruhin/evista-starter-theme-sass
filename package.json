{
  "name": "evista-starter-theme-sass",
  "version": "7.0.0",
  "description": "Evista Starter Theme with Sass for Wordpress",
  "main": "index.js",
  "scripts": {
    "dev": "npm rebuild node-sass && node node_modules/webpack-cli/bin/webpack.js --config webpack.config.js",
    "watch": "npm rebuild node-sass && node node_modules/webpack-cli/bin/webpack.js --watch --config webpack.config.js",
    "build": "node node_modules/webpack-cli/bin/webpack.js --optimize-minimize --config webpack.production.config.js"
  },
  "repository": "https://github.com/succli/evista-starter-theme-sass.git",
  "author": "Schutzbach Zsolt <schutzbach.zsolt@e-vista.hu>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.22.1",
    "babel-loader": "^7.0",
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.28",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.0",
    "node-sass": "^4.5.0",
    "sass-loader": "^6.0",
    "standard": "^11.0",
    "style-loader": "^0.20",
    "uglifyjs-webpack-plugin": "^1.2.2",
    "url-loader": "^1.0",
    "webpack": "^4.0",
    "webpack-cli": "^2.0.10"
  },
  "dependencies": {
    "bootstrap": "^4.0",
    "font-awesome": "^4.7.0",
    "imports-loader": "^0.8",
    "jquery": "^3.0",
    "popper.js": "^1.13"
  }
}
